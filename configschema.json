{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "channelId": {
            "type": "string"
        },
        "token": {
            "type": "string"
        },
        "interval": {
            "type": "number",
            "default": 1000
        },
        "ignoreUserIds": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
        "channelId",
        "token"
    ]
}